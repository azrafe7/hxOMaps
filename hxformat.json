{
  "disableFormatting": false,
  "excludes": [
    "IOrderedMap.hx"
  ],
  "emptyLines": {
    "afterLeftCurly": "keep",
    "afterPackage": 1,
    "maxAnywhereInFile": 2,
    "abstractEmptyLines": {
      "beginType": 1
    },
    "classEmptyLines": {
      "beginType": 1
    },
    "interfaceEmptyLines": {
      "beginType": 1
    }
  },
  "whitespace": {
    "typeCheckColonPolicy": "none",
    "typeHintColonPolicy": "none"
  },
  "indentation": {
    "character": "  ",
    "conditionalPolicy": "alignedDecrease"
  }
}